<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<description>controller manager bean</description>
		<managed-bean-name>controllerManageBean</managed-bean-name>
		<managed-bean-class>com.agsnasoft.managebean.ControllerManageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>persona</property-name>
			<property-class>com.agsnasoft.java.model.Persona</property-class>
			<value>#{persona} </value>
		</managed-property>
		<managed-property>
			<property-name>navigationBean</property-name>
			<property-class>com.agsnasoft.managebean.NavigationBean</property-class>
			<value>#{navigationBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>persona</managed-bean-name>
		<managed-bean-class>com.agsnasoft.java.model.Persona</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navigationBean</managed-bean-name>
		<managed-bean-class>com.agsnasoft.managebean.NavigationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
